{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "no-plusplus": "off",
    "no-tabs": "off",
    "no-alert":"off",
    "prefer-destructuring":"off",
    "indent": [2, "tab"],
    "no-param-reassign": "off",
    "eol-last": "off",
    "linebreak-style": "off"
  }
}